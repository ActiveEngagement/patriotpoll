{
  "name": "patriotpoll",
  "version": "0.1.63",
  "main": "dist/PatriotPoll.umd.min.js",
  "module": "src/index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build-lib && npm run build-wc -- --no-clean",
    "build-lib": "vue-cli-service build --inline-vue --target lib --name PatriotPoll src/Components/Poll/PollLoader.vue",
    "build-wc": "vue-cli-service build --inline-vue --target wc --name patriot-poll src/Components/Poll/PollLoader.vue",
    "lint": "vue-cli-service lint",
    "fix": "vue-cli-service lint --fix",
    "pre-release": "npm run build; git add . -A; git commit -m 'pre-release commit'",
    "release-patch": "npm run pre-release && npm version patch && npm run release;",
    "release-minor": "npm run pre-release && npm version minor && npm run release;",
    "release-major": "npm run pre-release && npm version major && npm run release;",
    "release": "npm publish; git add . -A; git commit -m 'post-release commit'; git push --tags origin"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "core-js": "^3.4.4",
    "d3": "^5.15.0",
    "moment": "^2.24.0",
    "randomcolor": "^0.5.4",
    "vue": "^2.6.10",
    "vue-interface": "^0.4.54",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.13.1",
    "postcss-prefix-selector": "^1.7.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
